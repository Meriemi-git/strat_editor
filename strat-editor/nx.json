{
  "npmScope": "strat-editor",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "frontend": {
      "tags": ["scope:front"]
    },
    "backend": {
      "tags": ["scope:back"]
    },
    "data": {
      "tags": ["scope:shared"]
    },
    "drawing-editor": {
      "tags": ["scope:front"]
    }
  }
}
